{
    "name": "tektite-console",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/TurtleStoffel/tektite-console"
    },
    "scripts": {
        "dev": "NODE_ENV=development bun --hot src/index.tsx",
        "start": "NODE_ENV=production bun src/index.tsx",
        "build": "bun run build.ts",
        "db:generate:local": "drizzle-kit generate --config=drizzle.local.config.ts",
        "db:migrate:local": "drizzle-kit migrate --config=drizzle.local.config.ts",
        "format": "biome format --write src/",
        "lint": "biome check src/",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@openai/codex-sdk": "^0.98.0",
        "@opencode-ai/sdk": "^1.2.5",
        "openai": "^6.3.0",
        "@tanstack/react-query": "^5.90.20",
        "@xterm/addon-fit": "^0.10.0",
        "bun-plugin-tailwind": "^0.1.2",
        "bun-pty": "^0.4.0",
        "d3-force": "^3.0.0",
        "daisyui": "^5.5.18",
        "drizzle-orm": "^0.44.7",
        "madge": "^8.0.0",
        "react": "^19.2.4",
        "react-dom": "^19.2.4",
        "react-router-dom": "^7.13.0",
        "tailwindcss": "^4.1.18",
        "typescript-result": "^3.5.2",
        "xterm": "^5.3.0",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.3.14",
        "@types/bun": "latest",
        "@types/d3-force": "^3.0.10",
        "@types/react": "^19.2.13",
        "@types/react-dom": "^19.2.3",
        "drizzle-kit": "^0.31.4",
        "knip": "^5.83.1",
        "supabase": "^2.76.6",
        "typescript": "^5.9.3"
    }
}
